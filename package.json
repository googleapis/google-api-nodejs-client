{
  "nyc": {
    "exclude": [
      "build/src/apis",
      "build/test"
    ]
  },
  "license": "Apache-2.0",
  "dependencies": {
    "google-auth-library": "^1.1.0",
    "qs": "^6.5.1",
    "string-template": "1.0.0",
    "uuid": "^3.1.0"
  },
  "files": [
    "LICENSE",
    "README.md",
    "build/src",
    "package.json"
  ],
  "semistandard": {
    "ignore": [
      "apis",
      "templates/*"
    ],
    "globals": [
      "after",
      "afterEach",
      "before",
      "beforeEach",
      "describe",
      "it"
    ]
  },
  "repository": {
    "url": "https://github.com/google/google-api-nodejs-client.git",
    "type": "git"
  },
  "name": "googleapis",
  "contributors": [
    {
      "email": "jbd@google.com",
      "name": "Burcu Dogan"
    },
    {
      "email": "jasonall@google.com",
      "name": "Jason Allor"
    },
    {
      "email": "jason.dobry@gmail.com",
      "name": "Jason Dobry"
    },
    {
      "email": "ryanseys@google.com",
      "name": "Ryan Seys"
    },
    {
      "email": "tbetbetbe@google.com",
      "name": "Tim Emiola"
    },
    {
      "email": "beckwith@google.com",
      "name": "Justin Beckwith"
    }
  ],
  "version": "26.0.1",
  "scripts": {
    "posttest": "npm run check && npm run license-check",
    "test": "npm run cover",
    "doc": "jsdoc -c jsdoc-conf.json",
    "prepare": "npm run compile",
    "build-tools": "tsc -p tsconfig.tools.json",
    "compile": "tsc -p .",
    "check-samples": "semistandard \"samples/**/*.js\"",
    "generate-apis": "npm run build-tools && node build/src/scripts/generate.js && clang-format -i -style='{Language: JavaScript, BasedOnStyle: Google, ColumnLimit: 80}' src/apis/**/*.ts",
    "cover": "nyc --cache mocha build/test/ --recursive -t 10000 -S -R spec --require intelli-espower-loader && nyc report --reporter=html",
    "pretest": "npm run compile",
    "clean": "gts clean",
    "check": "gts check && npm run check-samples",
    "fix": "gts fix && npm run fix-samples",
    "fix-samples": "semistandard --fix \"samples/**/*.js\"",
    "license-check": "jsgl --local ."
  },
  "author": "Google Inc.",
  "keywords": [
    "google",
    "api",
    "google apis",
    "client",
    "client library"
  ],
  "description": "Google APIs Client Library for Node.js",
  "main": "./build/src/index.js",
  "types": "./build/src/index.d.ts",
  "engines": {
    "node": ">=4.0"
  },
  "devDependencies": {
    "@types/minimist": "1.2.0",
    "@types/mkdirp": "0.5.2",
    "@types/mocha": "2.2.48",
    "@types/ncp": "^2.0.1",
    "@types/nock": "^9.1.2",
    "@types/node": "9.4.5",
    "@types/nunjucks": "^3.0.0",
    "@types/p-queue": "^2.3.0",
    "@types/pify": "^3.0.0",
    "@types/qs": "^6.5.1",
    "@types/rimraf": "^2.0.2",
    "@types/source-map-support": "^0.4.0",
    "@types/string-template": "^1.0.2",
    "@types/tmp": "0.0.33",
    "@types/uuid": "^3.4.3",
    "axios": "^0.17.1",
    "clang-format": "^1.2.2",
    "codecov": "^3.0.0",
    "gts": "0.5.3",
    "ink-docstrap": "1.3.2",
    "intelli-espower-loader": "1.0.1",
    "js-green-licenses": "^0.4.0",
    "jsdoc": "3.5.5",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "mocha": "5.0.1",
    "nconf": "^0.10.0",
    "ncp": "^2.0.0",
    "nock": "9.1.6",
    "nunjucks": "^3.0.1",
    "nyc": "11.4.1",
    "opn": "5.2.0",
    "p-queue": "^2.3.0",
    "pify": "^3.0.0",
    "rimraf": "2.6.2",
    "semistandard": "12.0.0",
    "source-map-support": "0.5.3",
    "tmp": "0.0.33",
    "typescript": "2.6.2"
  }
}
